{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
    {
      "endOfLine": "auto"
    }
    ]
  },
  "parser":"@babel/eslint-parser",
  "parserOptions": {
    "babelOptions": {
       "presets": ["@babel/preset-react"]
    },
   }
}
